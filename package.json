    "express": "^4.16.4",
    "glob": "^7.1.3",
    "graphql": "^14.0.2",
    "graphql-iso-date": "^3.6.1",
    "graphql-tools": "^4.0.3",
    "handlebars": "^4.1.0",
    "http-proxy-middleware": "^0.19.1",
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.11",
    "micromatch": "^3.1.10",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "node-cache": "^4.2.0",
    "opn": "^5.4.0",
    "rc": "^1.2.8",
    "sift": "^7.0.1",
    "simple-git": "^1.107.0",
    "slugify": "^1.3.4",
    "wkhtmltopdf": "^0.3.4"
  },
